{
  "name": "@gc-utils/release",
  "version": "0.1.3",
  "description": "gc-utils release script",
  "bin": {
    "release": "src/cli.mjs"
  },
  "type": "module",
  "main": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "license": "MIT",
  "author": "Ghislain B.",
  "homepage": "https://github.com/ghiscoding/release",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghiscoding/release.git",
    "directory": "packages/copyfiles"
  },
  "bugs": {
    "url": "https://github.com/ghiscoding/release/issues"
  },
  "scripts": {
    "build": "tsc --outDir dist",
    "release": "node scripts/release.mjs",
    "preview:copy": "node dist/cli.js test-copyin test-copyout --flat --verbose"
  },
  "dependencies": {
    "conventional-changelog": "^6.0.0",
    "rimraf": "^6.0.1",
    "semver": "^7.7.1",
    "tinyexec": "^0.3.2",
    "tinyglobby": "^0.2.10",
    "tinyrainbow": "^2.0.0",
    "untildify": "^5.0.0",
    "yargs": "^17.7.2"
  }
}